;+
;
;
;
;   Read the quanta file and make a structure
;
;
;
;
;-


pro sb99_quanta, str, model, head

readcol, model, time, q_hi, frac_hi, q_hei, frac_hei, q_heii, frac_heii, logl 

str={TIME:time,Q_HI:q_hi,F_HI:frac_hi,Q_HEI:q_hei,F_HEI:frac_hei,$
     Q_HEII:q_heii, F_HEII:frac_heii, LOG_L:logl}

end
